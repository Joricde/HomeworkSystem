<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.homeworksystem.DAO.HomeworkDAO">

    <select id="getHomeWorkByStu" resultType="com.homeworksystem.entity.HomeWork">
        select *
        from homework
        where student_no = #{student_no}
    </select>

</mapper>